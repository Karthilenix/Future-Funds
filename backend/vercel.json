{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "MONGODB_URI": "@mongodb-uri",
        "JWT_SECRET": "@jwt-secret"
    }
}