{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "MONGODB_URI": "@mongodb-uri",
        "JWT_SECRET": "@jwt-secret"
    }
}